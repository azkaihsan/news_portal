---
import NewsPortalWrapper from '../../components/NewsPortalWrapper.jsx';
import '../../styles/global.css';
export const prerender = false;
const { categoryName } = Astro.params;
---
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="icon" type="image/png" href="/pelita-logo.png" />
  <title>{categoryName ? `${categoryName.charAt(0).toUpperCase() + categoryName.slice(1)} News` : 'Category News'} - News Portal</title>
</head>
<body>
  <NewsPortalWrapper client:only="react" />
</body>
</html>
